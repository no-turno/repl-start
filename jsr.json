{
    "name": "@no-turno/repl-start",
    "version": "0.2.2",
    "description": "Utils for dev starters",
    "exports": {
        ".": "./src/index.ts"
    },
    "compilerOptions": {
        "lib": [
          "esnext"
        ],
        "types": ["bun"]
      },
    "publish": {
        "include": [
            "jsr.json",
            "README.md",
            "src/index.ts",
            "src/types.ts"
        ],
        "exclude": [
            "src/.examples.ts",
            "src/.snap.ts",
            "bunfig.toml",
            ".config",
            "node_modules",
            "dist",
            "bundle.ts",
            "tests"
        ]
    },
    "unstable": [
        "sloppy-imports"
    ]
}